<h1 class="page-title">All Users</h1>

<table class="table table-striped col-md-12" id="movies">
  <thead>
    <tr>
      <th>Email</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= user.options %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>